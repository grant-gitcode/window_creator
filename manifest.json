{
  "name": "Window Creator",
  "description": "This extension creates a new window from preexisting windows",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "browser_action.html"
  },
  "commands": {
    "asfd": {
      "suggested_key": {"default": "Ctrl+Shift+Z"},
      "description": "blah"
    },
    "ffaf": {
      "suggested_key": {"default": "Alt+Shift+C"},
      "description": "bleh"
    },
    "_execute_browser_action": {
      "suggested_key": {"default": "Alt+Shift+O"},
      "description": "blih"
    }
  },
   "permissions": ["tabs","storage"]
}
